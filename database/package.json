{
	"name": "real-time-sport-dashboard-database",
	"version": "1.0.0",
	"description": "",
	"main": "",
	"scripts": {
		"new:migration": "cross-env ts-node './scripts/create-migration-cli.ts'",
		"run:latest:migration": "cross-env pnpx knex migrate:up --knexfile './knexfile.ts'",
		"run:all:migration": "cross-env pnpx knex migrate:latest --knexfile './knexfile.ts'",
		"rollback:latest:migration": "cross-env pnpx knex migrate:rollback --knexfile './knexfile.ts'",
		"list:migration": "cross-env pnpx knex migrate:list --knexfile './knexfile.ts'",
    	"undo:latest:migration": "cross-env pnpx knex migrate:down --knexfile './knexfile.ts'",
		"reset:database": "cross-env ts-node './initial_migration.ts' && pnpm run:all:migration"
	},
	"keywords": [],
	"author": "Louis DANIEL",
	"license": "ISC",
	"packageManager": "pnpm@10.17.1",
	"engines": {
		"node": ">=22.14.0",
		"pnpm": ">=10.17.1"
	},
	"dependencies": {
		"@dotenvx/dotenvx": "^1.51.0",
		"knex": "^3.1.0",
		"kysely": "^0.28.7",
		"pg": "^8.16.3"
	},
	"devDependencies": {
		"@types/node": "^24.6.2",
		"@types/pg": "^8.15.5",
		"cross-env": "^10.1.0",
		"eslint": "^9.37.0",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.9.3"
	}
}
